{
    "compilerOptions": {
      "moduleResolution": "node16",
      "module": "Node16",
      "target": "ES2020",
      "outDir": "out",
      "lib": [
        "es6",
        "dom"
      ],
      "sourceMap": true,
      "rootDir": ".",
      "strict": true,
      "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
    },
    "exclude": [
      "node_modules",
      ".vscode-test"
    ],
    "include": [
    "src/**/*",
    "test/**/*"
  ],
  }
  